<% include ./partials/header.ejs %>
<div class="show w-full min-h-screen px-12 py-10 text-black">
    <div class="cards flex flex-wrap gap-10">
        <% user.posts.forEach(function(elem){ %>
            <div class="actualcard w-60">
              <div class="card2 w-full h-60 bg-sky-200  overflow-hidden">
                <img class="w-full h-full object-cover" src="/images/uploads/<%= elem.postimage %>" alt="">
              </div>
              <a href="/image/<%= elem._id %>" class="mt-2"><%= elem.title %></a>
            </div>
          <% }) %>
<% include ./partials/footer.ejs %>